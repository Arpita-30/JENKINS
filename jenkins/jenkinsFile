pipeline{
    //agent any
    agent { docker { image 'maven:3.6.3'}}
    stages{
        stage('build'){
            steps{
                echo "mvn --version"
                echo "build"
            }
        }
        stage('test'){
            steps{
                echo "test"
            }
        }
        stage('integation test'){
            steps{
                echo "Intregation test"
            }
        }
    }
    
}